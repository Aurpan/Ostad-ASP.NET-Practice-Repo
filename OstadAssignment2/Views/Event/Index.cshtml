@model List<EventViewModel>

<h2>@ViewData["header"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Date</th>
            <th>Location</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var eventItem in Model)
        {
            <tr>
                <td>@eventItem.Name</td>
                @* <td>@eventItem.Date.ToString("MM/dd/yyyy")</td> *@
                <td>@eventItem.Date</td>
                <td>@eventItem.Address</td>
                <td>@eventItem.Description</td>
            </tr>
        }
    </tbody>

